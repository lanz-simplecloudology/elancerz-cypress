{
  "name": "elancerz",
  "version": "1.0.0",
  "description": "Cypress Automation for Elancerz",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "npm cypress run",
    "run:stg": "npx cypress run --config-file cypress/config/stg.json --spec cypress/integration/e2e/**/*",
    "run:dev": "npx cypress run --config-file cypress/config/dev.json --spec cypress/integration/e2e/**/*",
    "open:stg": "npx cypress open --config-file cypress/config/stg.json",
    "open:dev": "npx cypress open --config-file cypress/config/dev.json",
    "git:stg": "npx cypress run --spec cypress/integration/CI/**/* configFile=git",
    "dev:stg": "npx cypress run --spec cypress/integration/CI/**/* --env user=$USERNAME,password=$PASSWORD,url=https://dev.elancerz.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lanz-simplecloudology/elancerz-cypress.git"
  },
  "author": "Lanz Gementiza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lanz-simplecloudology/elancerz-cypress/issues"
  },
  "homepage": "https://github.com/lanz-simplecloudology/elancerz-cypress#readme",
  "devDependencies": {
    "cypress-xpath": "^1.6.2"
  },
  "dependencies": {
    "cypress": "^7.7.0",
    "cypress-real-events": "^1.5.0"
  }
}
